      "name": "build",
      "type": "node",
      "program": "${workspaceFolder}/examples/postcss/build.ts",
      "cwd": "${workspaceFolder}/examples/postcss",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "-A",
        "--inspect-wait"
      ],
      "attachSimplePort": 9229
    },
    {
      "request": "launch",
      "name": "run",
      "type": "node",
      "program": "${workspaceFolder}/examples/postcss/run.ts",
      "cwd": "${workspaceFolder}/examples/postcss",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "-A",
        "--inspect-wait"
      ],
      "attachSimplePort": 9229
    },
    {
      "request": "launch",
      "name": "test",
      "type": "node",
      "program": ".",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "test",
        "-A",
        "--inspect-wait"
      ],
      "attachSimplePort": 9229
    }
  ]
